const jsonData = [
  { "customer": "customer1", "date": "1 july" },
  { "customer": "customer2", "date": "2 july" },
  { "customer": "customer1", "date": "3 july" }
];

// Extract unique customer names
const customers = [];
jsonData.forEach(invoice => customers.push(invoice.customer));
const uniqueCustomers = new Set(customers);

// Get the number of unique customers
const numCustomers = uniqueCustomers.size;

console.log(numCustomers); // Output: 2